{
  "name": "frontend",
  "version": "0.1.0", 
  "private": true,
  "scripts": {
    "predev": "npx kill-port 3000 || echo 'Port cleaning attempted'",
    "dev": "cross-env PORT=3000 NEXT_PUBLIC_BASE_URL=http://localhost:3000 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhook --events checkout.session.completed,payment_intent.succeeded,payment_intent.payment_failed",
    "stripe:test": "stripe trigger checkout.session.completed",
    "stripe:check": "stripe --version && stripe config --list",
    "dev:full": "concurrently \"npm run dev\" \"npm run stripe:listen\"",
    "webhook:test": "curl -X POST http://localhost:3000/api/webhook -H \"Content-Type: application/json\" -d '{\"test\": true}'",
    "env:check": "node -e \"console.log('STRIPE_SECRET_KEY:', process.env.STRIPE_SECRET_KEY ? '✓ Set' : '✗ Missing'); console.log('STRIPE_WEBHOOK_SECRET:', process.env.STRIPE_WEBHOOK_SECRET ? '✓ Set' : '✗ Missing');\""
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@tailwindcss/typography": "^0.5.16",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "micro": "^10.0.1",
    "mongodb": "^6.17.0",
    "next": "15.3.4",
    "openai": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "stripe": "^18.2.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.3",
    "typescript": "5.8.3",
    "concurrently": "^8.2.2"
  }
}